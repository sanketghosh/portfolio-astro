---
interface LinkProps {
  target: string;
  href: string;
  label: string;
}

const { target = "_self", label, href } = Astro.props as LinkProps;
---

<a
  href={href}
  target={target}
  class="text-sm font-extrabold underline underline-offset-4 transition-all hover:text-black/70"
  >{label}</a
>
